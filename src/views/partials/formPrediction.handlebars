<main class="main-container">
  <h1>Enviar prediccion🍾🏆🍾</h1>
  <form id="formulario">
    <label for="anio">Año:</label>
    <input type="number" id="anio" value="2025" disabled required />

    <label for="carrera">Nombre de la carrera:</label>
    <input type="text" id="carrera" list="sugerencias" placeholder="Ej: Miami" required />
    <datalist id="sugerencias"></datalist>

    <label for="pilotoP1">Piloto P1🥇</label>
    <input type="text" id="pilotoP1" list="sugerenciasP1" required />
    <datalist id="sugerenciasP1"></datalist>

    <label for="pilotoP2">Piloto P2🥈</label>
    <input type="text" id="pilotoP2" list="sugerenciasP2" required />
    <datalist id="sugerenciasP2"></datalist>

    <label for="pilotoP3">Piloto P3🥉</label>
    <input type="text" id="pilotoP3" list="sugerenciasP3" required />
    <datalist id="sugerenciasP3"></datalist>

    <button type="submit">Enviar</button>
  </form>
  <div id="resultado"></div>
</main>

<script defer src="/js/prediccion.js"></script>